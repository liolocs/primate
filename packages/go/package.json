{
  "name": "@primate/go",
  "version": "0.1.6",
  "description": "Primate Go backend",
  "homepage": "https://primate.run/docs/backend/go",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.sum",
    "/lib/**/*.mod",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/go"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/assert": "^0.3.1",
    "@rcompat/cli": "^0.10.3",
    "@rcompat/env": "^0.9.0",
    "@rcompat/fs": "^0.21.1",
    "@rcompat/runtime": "^0.6.0",
    "@rcompat/stdio": "^0.11.0",
    "@rcompat/string": "^0.10.0"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
